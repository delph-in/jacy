 [ ORTH <! "に従い" !>,
   SYNSEM.LOCAL [ CAT.HEAD.PTYPE nitsuite,
                    CONT.RELS <! [ PRED "nishitagai" ] !> ] ].

kadouka-comp-naarg := comp-int-lex &
