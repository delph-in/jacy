                    CONT.RELS <! [ PRED "ga_prep" ] !> ] ].

ga3 := topic-vmod-p-lex &
 [ ORTH <! "が" !>,
   SYNSEM.LOCAL [ CAT [ HEAD [ MOD < [ LOCAL [ CAT [ HEAD.EMPTY -,
                                                     VAL [ SUBJ null,
                                                           UNSAT - ] ],
                                               CONT.HOOK.XARG full_ref-ind ] ] >,
                                 PTYPE topic ],
                          VAL.COMPS.FIRST.LOCAL.CAT.HEAD noun_head ],
                    CONT.RELS <! [ PRED "ga_prep" ] !> ] ].

