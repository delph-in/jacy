   SYNSEM.LOCAL [ CAT.HEAD [ MOD < [ LOCAL.CAT.HEAD.MAIN-PRD + ] >,
                                 PTYPE de ],
                    CONT.RELS <! [ PRED "de" ] !> ] ].

to-comp-varg := comp-prpstn-lex &
 [ ORTH <! "と" !>,
   SYNSEM.LOCAL.CAT [ HEAD [ CASE to,
                                 PTYPE to ],
                          VAL.COMPS.FIRST.LOCAL.CAT.HEAD verb_head & [ MAIN-PRD -,
                                                                     SMOD smod,
                                                                     VN - ] ] ].

